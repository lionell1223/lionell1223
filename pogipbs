function c9()
IMmenu = gg.multiChoice({
"MB OPTIC"
}, nil, "Telegram: @dosshit")
if IMmenu == nil then 
HOME()
else
if IMmenu[1] == true then 
aim() 
end
end
YACHIRU = -1
end

function aim()
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("1.0F;4.59177481e-39F:17", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
local results = gg.getResults(10)
if #results > 0 then
for i, v in ipairs(results) do
if v.value == 1.0 then
v.value = 1.003
v.freeze = true
end
end
gg.addListItems(results)
gg.toast("MB OPTIC ON")
else
gg.toast("Not Found !!!")
end
gg.clearResults()
end

