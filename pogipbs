function c1()
gg.clearResults()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber("1.75;1.03499996662", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.getResults(1000)
gg.editAll("0.1", gg.TYPE_FLOAT)
gg.toast("𝙽𝙾 𝚁𝙴𝙲𝙾𝙸𝙻 𝚅𝟷(stable)activate")
gg.clearResults()
gg.setVisible(false)
gg.clearResults()
end

